    "svelte-check": "^3.6.0",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
}
